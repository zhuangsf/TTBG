<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="@dimen/stroke_width_shape" android:color="@color/stroke" />
            <corners android:bottomRightRadius="@dimen/dialog_base_corners" />
        </shape>
    </item>
    <item android:left="@dimen/stroke_width_large" android:top="@dimen/stroke_width_large">
        <shape android:shape="rectangle">
            <solid android:color="@color/click_bg_gray" />
            <corners android:bottomRightRadius="@dimen/dialog_base_corners" />
        </shape>
    </item>
</layer-list>